{
    "name": "react-personal-page",
    "version": "1.0.0",
    "description": "Personal page built in react",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean:build && webpack --mode production",
        "build:ts": "tsc --noEmit -p tsconfig.json",
        "clean:build": "rm -rf dist",
        "clean:dependencies": "rm -rf node_modules/ package-lock.json",
        "prettier": "prettier --list-different \"src/**/*.{ts,tsx}\"",
        "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
        "start": "node index.js",
        "start:dev": "webpack-dev-server --open --mode development"
    },
    "author": "Carles Capellas <capellas.carles@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@types/react": "16.9.41",
        "@types/react-dom": "16.9.8",
        "@types/react-router-dom": "5.1.5",
        "awesome-typescript-loader": "5.2.1",
        "css-loader": "3.6.0",
        "file-loader": "6.0.0",
        "html-loader": "1.1.0",
        "html-webpack-plugin": "4.3.0",
        "node-sass": "4.14.1",
        "prettier": "2.0.5",
        "sass-loader": "8.0.2",
        "style-loader": "1.2.1",
        "ts-node": "8.10.2",
        "typescript": "3.9.5",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.0"
    },
    "dependencies": {
        "express": "4.17.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-router-dom": "5.2.0"
    }
}
